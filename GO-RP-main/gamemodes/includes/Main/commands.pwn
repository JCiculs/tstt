	if(moneys < 0)
	{
		SendClientMessageEx(playerid, COLOR_GRAD1, "That is not enough.");
		return 1;
	}
	if(GetPlayerCash(playerid) < moneys)
	{
		SendClientMessageEx(playerid, COLOR_GRAD1, "Ban khong co du tien.");
