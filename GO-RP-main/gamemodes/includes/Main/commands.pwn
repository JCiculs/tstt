	new string[128], giveplayerid;
	if(sscanf(params, "u", giveplayerid)) return SendClientMessageEx(playerid, COLOR_GREY, "SU DUNG: /choxemgiayphep [player]");

	if(IsPlayerConnected(giveplayerid))
	{
