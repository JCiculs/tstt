						{
							foreach(new p : Player) SetTimerEx("AutomaticGateTimer", 1000, false, "ii", p, i);
						}
						format(string, sizeof(string), "%s has locked gate ID %d.", GetPlayerNameEx(playerid), i);
						Log("logs/gedit.log", string);
					}
				}
			}
		}
	}
	return 1;
}

