		new iOrderState = Businesses[iBusinessID][bOrderState];
		if (!iOrderState)
		{
			SendClientMessageEx(playerid, COLOR_WHITE, "Cua hang cua ban chua bao giot dat hang tiep te.");
			return 1;
		}
		else
		{
			new string[128];
