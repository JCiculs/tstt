CMD:rccam(playerid, params[])
{
	if(PlayerInfo[playerid][pRccam] > 0)
	{
		new string[128];
		if(GetPVarInt(playerid, "rccam") == 0)
		{
			if(IsPlayerInAnyVehicle(playerid))
			{
				SendClientMessageEx(playerid, COLOR_GRAD1, "Ban phai di bo de dat RCCam!");
